{
  "name": "ember-polymer",
  "version": "2.2.0",
  "description": "Allows you to use Polymer in your Ember application.",
  "scripts": {
    "build": "ember build",
    "test:all": "ember try:each",
    "test-node": "./node_modules/mocha/bin/_mocha test/**/*-test.js",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --max-warnings 0 ./*.js addon app config tests",
    "start": "ember server",
    "test": "COVERAGE=true ember test",
    "check-coverage": "istanbul check-coverage coverage/coverage-final.json --statements 100 --functions 100 --branches 100 --lines 100",
    "commit": "git-cz",
    "posttest": "npm run report-coverage && npm run check-coverage",
    "report-coverage": "istanbul report --include=coverage/coverage-final.json text",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.3.1",
    "@ember/optional-features": "^0.6.4",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "bower": "^1.8.4",
    "broccoli-asset-rev": "^2.7.0",
    "codeclimate-test-reporter": "^0.5.0",
    "commitizen": "^3.0.5",
    "console-ui": "^2.1.0",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "ember-cli": "~3.7.1",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-code-coverage": "^1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^1.1.1",
    "ember-cli-deploy-git": "^1.3.1",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.2",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.7.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^0.2.23",
    "eslint-config-bbva": "^2.0.3",
    "eslint-plugin-ember": "^5.4.0",
    "eslint-plugin-node": "^7.0.1",
    "greenkeeper-lockfile": "^1.12.0",
    "husky": "^1.3.1",
    "istanbul": "^0.4.5",
    "loader.js": "^4.7.0",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "qunit-dom": "^0.8.0",
    "semantic-release": "^15.13.3"
  },
  "keywords": [
    "ember-addon",
    "polymer",
    "web components",
    "custom elements",
    "polymer-project"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dunnkers/ember-polymer.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jeroen Overschie",
    "email": "info@periodpoint.com",
    "url": "https://github.com/dunnkers"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "broccoli-merge-trees": "^3.0.0",
    "broccoli-plugin": "^1.3.0",
    "ember-cli-babel": "^7.1.2",
    "ember-cli-htmlbars": "^3.0.0",
    "fs-extra": "^6.0.0",
    "parse5": "^4.0.0",
    "path": "^0.12.7",
    "polymer-analyzer": "^2.3.0",
    "polymer-bundler": "^3.1.1"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://dunnkers.github.io/ember-polymer/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
