{
  "name": "ember-cli-polymer-bundler",
  "version": "1.0.0",
  "description": "Allows you to use Polymer in your Ember application.",
  "license": "MIT",
  "author": "BBVAEngineering",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "index.js",
    "lib",
    "polyfills"
  ],
  "scripts": {
    "build": "ember build --environment=production",
    "start": "ember server",
    "lint": "npm run lint:js && npm run lint:hbs",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --max-warnings 0 .",
    "pretest": "npm run clean-coverage",
    "test": "npm run test:ember && npm run test:node",
    "test:ember": "COVERAGE=true ember test",
    "test:node": "nyc mocha --recursive 'node-tests/**/*-test.js'",
    "posttest": "npm run merge-coverage && npm run report-coverage && npm run check-coverage",
    "clean-coverage": "rm -rf coverage",
    "merge-coverage": "cp coverage/ember/coverage-final.json coverage/ember.json && cp coverage/node/coverage-final.json coverage/node.json && nyc merge coverage coverage/coverage-final.json",
    "report-coverage": "istanbul report --include=coverage/coverage-final.json text",
    "check-coverage": "istanbul check-coverage coverage/coverage-final.json --statements 98 --functions 97 --branches 93 --lines 98",
    "validate-coverage": "test \"$(cat coverage/coverage-summary.json | json total.lines.total)\" -gt 0",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@wctools/polymer-project-builder": "^1.0.0",
    "autoprefixer": "^9.4.7",
    "broccoli-caching-writer": "^3.0.3",
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^3.0.0",
    "broccoli-plugin": "^1.3.0",
    "deep-extend": "^0.6.0",
    "ember-cli-babel": "^7.18.0",
    "fs-extra": "^8.0.0",
    "html-postcss": "^0.1.2",
    "parse5": "^5.1.0",
    "path": "^0.12.7",
    "polymer-analyzer": "^2.3.0",
    "polymer-bundler": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "bower": "^1.8.4",
    "commitizen": "^4.0.3",
    "console-ui": "^3.0.0",
    "cz-conventional-changelog": "^3.1.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "^3.16.2",
    "ember-cli-app-version": "3.2.0",
    "ember-cli-code-coverage": "^1.0.0-beta.9",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^4.2.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-template-lint": "^2.5.0",
    "ember-source": "~3.16.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-bbva": "^3.2.2",
    "eslint-plugin-bbva": "^1.3.3",
    "eslint-plugin-ember": "^7.10.1",
    "eslint-plugin-node": "^11.0.0",
    "husky": "^4.2.4",
    "istanbul": "^0.4.5",
    "json": "^9.0.6",
    "loader.js": "^4.7.0",
    "mocha": "^5.0.0",
    "nyc": "^13.1.0",
    "qunit-dom": "^1.0.0",
    "rimraf": "^2.6.3",
    "semantic-release": "^17.0.4"
  },
  "keywords": [
    "ember-addon",
    "polymer",
    "web components",
    "custom elements",
    "polymer-project"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BBVAEngineering/ember-cli-polymer-bundler.git"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://dunnkers.github.io/ember-polymer/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
