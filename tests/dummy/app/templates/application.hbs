<paper-drawer-panel>
  <paper-header-panel slot="drawer">
    <paper-toolbar slot="header">
      <div slot="top">Drawer</div>
    </paper-toolbar>
    <paper-listbox selected="0">
      <paper-item>Item 1</paper-item>
      <paper-item>Item 2</paper-item>
    </paper-listbox>
  </paper-header-panel>
  <paper-header-panel slot="main">
    <paper-toolbar slot="header">
      <div slot="top">Example ember-polymer elements</div>
    </paper-toolbar>
    <div class="outlet">
      {{outlet}}
    </div>
  </paper-header-panel>
</paper-drawer-panel>
